<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		
		<bean class="converters.AdminToStringConverter" />
		<bean class="converters.StringToAdminConverter" />
		
		<bean class="converters.CategoryToStringConverter" />
		<bean class="converters.StringToCategoryConverter" />
		
		<bean class="converters.CreditCardToStringConverter" />
		<bean class="converters.StringToCreditCardConverter" />
		
		<bean class="converters.CrownToStringConverter" />
		<bean class="converters.StringToCrownConverter" />
		
		<bean class="converters.ExtraRewardToStringConverter" />
		<bean class="converters.StringToExtraRewardConverter" />
		
		<bean class="converters.ProjectToStringConverter" />
		<bean class="converters.StringToProjectConverter" />
		
		<bean class="converters.RewardToStringConverter" />
		<bean class="converters.StringToRewardConverter" />
		
		<bean class="converters.ContestToStringConverter" />
		<bean class="converters.StringToContestConverter" />
		
		<bean class="converters.SmsToStringConverter" />
		<bean class="converters.StringToSmsConverter" />
		
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.StringToCommentConverter" />
		
		<bean class="converters.SuperUserToStringConverter" />
		<bean class="converters.StringToSuperUserConverter" />
	</util:list>
	
</beans>